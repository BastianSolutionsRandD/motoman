
<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for es165d:
   - 6 joints

  Usage:
    robot_interface_streaming_es165d.launch robot_ip:=<value> controller:=<fs100|dx100>
-->
<launch>
	<arg name="robot_ip" doc="IP of controller" />
	
  <!-- controller: Controller name (fs100 or dx100) -->
  <arg name="controller" doc="Series of the controller (dx100, dx200, fs100 or yrc1000)"/>

	<rosparam command="load" file="$(find motoman_es165d_support)/config/joint_names_es165d.yaml" />

	<include file="$(find motoman_driver)/launch/robot_interface_streaming_$(arg controller).launch">
		<arg name="robot_ip"   value="$(arg robot_ip)" />
	</include>
</launch>
