<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_es165d" params="prefix">

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/visual/Base.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/collision/Base.stl"/>
            </geometry>
        </collision>
    </link>

    <link name="${prefix}S_axis">
        <visual>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/visual/S_axis.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/collision/S_axis.stl"/>
            </geometry>
        </collision>
    </link>

    <link name="${prefix}L_axis">
        <visual>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/visual/L_axis.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/collision/L_axis.stl"/>
            </geometry>
        </collision>
    </link>

    <link name="${prefix}U_axis">
        <visual>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/visual/U_axis.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/collision/U_axis.stl"/>
            </geometry>
        </collision>
    </link>

    <link name="${prefix}R_axis">
        <visual>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/visual/R_axis.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_es165d_support/meshes/collision/R_axis.stl"/>
            </geometry>
        </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1_s" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}S_axis"/>
        <origin xyz="0 0 0.302" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-2.9670" upper="2.9670" effort="212.66" velocity="6.5448"/>
    </joint>

    <joint name="${prefix}joint_2_l" type="revolute">
        <parent link="${prefix}S_axis"/>
        <child link="${prefix}L_axis"/>
        <origin xyz="0.285 -0.082 0.348" rpy="${pi/2} 0 ${pi}" />
        <axis xyz="0 0 1" />
        <limit lower="-2.9670" upper="2.9670" effort="212.66" velocity="6.5448"/>
    </joint>

    <joint name="${prefix}joint_3_u" type="revolute">
        <parent link="${prefix}L_axis"/>
        <child link="${prefix}U_axis"/>
        <origin xyz="0 1.150 -0.235" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-2.9670" upper="2.9670" effort="212.66" velocity="6.5448"/>
    </joint>

    <joint name="${prefix}joint_4_r" type="revolute">
        <parent link="${prefix}U_axis"/>
        <child link="${prefix}R_axis"/>
        <origin xyz="1.225 -0.25 317" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-2.9670" upper="2.9670" effort="212.66" velocity="6.5448"/>
    </joint>

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.330" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    </xacro:macro>
</robot>

